{
    "cleanUrls": true,
    "trailingSlash": false,
    "redirects": [
        {
            "source": "/welcome",
            "destination": "/",
            "permanent": false
        },
        {
            "source": "/map",
            "destination": "/energy-map",
            "permanent": false
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "SAMEORIGIN"
                }
            ]
        }
    ]
}
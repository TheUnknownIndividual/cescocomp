{
    "rewrites": [
        {
            "source": "/",
            "destination": "/welcome.html"
        },
        {
            "source": "/welcome",
            "destination": "/welcome.html"
        },
        {
            "source": "/map",
            "destination": "/index.html"
        },
        {
            "source": "/regulatory-framework",
            "destination": "/regulatory-framework.html"
        },
        {
            "source": "/solar-calculator",
            "destination": "/solar-calculator.html"
        },
        {
            "source": "/energy-law",
            "destination": "/energy-law.html"
        },
        {
            "source": "/law-electric-power",
            "destination": "/law-electric-power.html"
        },
        {
            "source": "/law-efficiency",
            "destination": "/law-efficiency.html"
        },
        {
            "source": "/law-environmental",
            "destination": "/law-environmental.html"
        },
        {
            "source": "/net-metering-decree",
            "destination": "/net-metering-decree.html"
        },
        {
            "source": "/policy-detail",
            "destination": "/policy-detail.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "SAMEORIGIN"
                }
            ]
        }
    ]
}